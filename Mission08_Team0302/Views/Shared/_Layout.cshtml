<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Mission08_Team0302</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css"/>
</head>
<body>

    <!-- Defines a navigation bar with specific styles (white background, border at the bottom, and margin at the bottom) -->
    <nav class="navbar navbar-expand-md bg-white border-bottom mb-3">
        <!-- A link to the 'Home' page-->
        <a class="navbar-brand" asp-controller="Home" asp-action="Index">Home</a>
        <!-- A link to 'Some Add Task-->
        <a class="navbar-brand" asp-controller="Home" asp-action="Create Tasks">Create Task</a>
        <!-- A link to 'Quadrant'Page-->
        <a class="navbar-brand" asp-controller="Home" asp-action="Quadrant">Quadrant</a>
    </nav>

<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>


<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>